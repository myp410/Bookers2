<%= render 'books/leftbar', user:@user %>

<h2>Users</h2>
<table>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="1"></th>
    </tr>
  </thead>
  <tbody>
    <% @user.each do |user| %>
    <tr>
      <td><%= image_tag get_profile_image(100,100) %></td>
      <td><%= user.name %></td>
      <%= link_to "Show", user_path(@user.id) %>
    </tr>
    <% end %>
  </tbody>
</table>